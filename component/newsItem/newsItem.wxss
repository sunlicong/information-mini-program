/* component/newsItem/newsItem.wxss */
@import "/app.wxss";
.feed-title {
    margin-left: 30rpx;
    margin-right: 30rpx;
    margin-top: 28rpx;
    font-size: 32rpx;
    color: #051426;
}

.feed-imgs {
    width: 100%;
    height: 140rpx;
    margin-top: 26rpx;
    margin-left: 15rpx;
    display: flex;
    align-items: center;
}

.feed-imgs .feed-img {
    width: 220rpx;
    height: 140rpx;
    margin-left: 15rpx;
}
.feed-bottom{
    width: 100%;
    height: 66rpx;
    line-height: 66rpx;
    font-size: 22rpx;
    color: #A3AEBA;
    display: flex;
    flex-direction: row;
}
.feed-bottom .feed-bottom-type{
    margin-left: 30rpx;
}