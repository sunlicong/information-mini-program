/* pages/editProfile/editProfile.wxss */
.myInfo {
    width: 100%;
    height: 230rpx;
    text-align: center;
    background-color: #fff;
}

.myInfo .cover {
    width: 120rpx;
    height: 120rpx;
    border-radius: 60rpx;
    margin-top: 35rpx;
    background-color: red;
}

.myInfo .nick {
    font-size: 32rpx;
    color: #A3AEBA;
    display: block;
}
.item{
    margin-top: 20rpx;
    height: 110rpx;
    display: flex;
    flex-direction: row;
    line-height: 110rpx;
    background-color: #fff;
    padding-left: 30rpx;
    padding-right: 30rpx;
    font-size: 32rpx;
    color: #051426;
}
.item .input{
    flex: 1;
    text-align: right;
    height: 110rpx;
    line-height: 110rpx;
}
.item .input_class{
    font-size: 32rpx;
    color:#A3AEBA;
}