/**index.wxss**/

.swiper {
    height: 240rpx;
}

.swiper-cover {
    width: 100%;
    height: 240rpx;
}

.height-90 {
    width: 100%;
    height: 90rpx;
    background-color: white;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.left {
    display: flex;
    align-items: center;
    margin-left: 30rpx;
    font-size: 32rpx;
    color: #051426;
}

.right {
    display: flex;
    align-items: center;
    margin-right: 30rpx;
}

.arrow {
    width: 12rpx;
    height: 21rpx;
    margin-left: 12rpx;
}

.margin-top-2 {
    margin-top: 2rpx;
}

.num-view {
    width: 100%;
    height: 143rpx;
    background-color: #fff;
    display: flex;
    justify-content: center;
}

.line {
    margin-top: 33rpx;
    width: 3rpx;
    height: 77rpx;
    background: #cddbec;
}

.num-item {
    width: 50%;
    height: 100%;
    float: left;
    display: flex;
    flex-direction: column;
    margin-left: 30rpx;
    justify-content: center;
    font-size: 28rpx;
    color: #051426;
}

.num-item .money {
    font-size: 32rpx;
    color: #fe6503;
}

.feed {
    background-color: #fff;
}
.my_mining{
    z-index: 11;
    position: fixed;
    width: 95rpx;
    height: 95rpx;
    bottom: 82rpx;
    right: 30rpx;
}
.non{
  margin-top: 250rpx;
}