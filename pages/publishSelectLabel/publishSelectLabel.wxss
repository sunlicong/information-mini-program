/* pages/publishSelectLabel/publishSelectLabel.wxss */
.container{
  padding-bottom: 100rpx;
}
.item{
  margin-top: 1rpx;
  width: 100%;
  height: 110rpx;
  background: #ffffff;
  display: flex;
  align-items: center;
}
.choose{
  margin-left: 30rpx;
  margin-right: 20rpx;
}
.nick{
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #051426;
}
.button{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  background: #0794FC;
  border: 0;
  border-radius: 0;
  font-family: PingFangSC-Medium;
  font-size: 36rpx;
  color: #FFFFFF;
}
.button::after{
  border: 0;
}